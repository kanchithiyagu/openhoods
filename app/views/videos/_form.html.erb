<%= simple_form_for(@video) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :avatar %>
  </div>
  <div class="form-inputs">
    <%= f.input :video %>
  </div>
  <div class="form-inputs">
    <%= f.input :user_id, :as => :hidden, :input_html => { :value => current_user.id } %>
  </div>
  <div class="form-inputs">
    <%= label_tag :vehicle_id  %>
    <%= select_tag 'vehicle_id', options_for_select(Vehicle.all.collect{|x| [x.title,x] },:prompt => '-------------' ) %>
  </div>


  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
