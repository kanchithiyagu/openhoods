<h2>Sign up</h2>
<%#raise params.inspect%>
<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :multipart => true }) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :name, input_html: { value: params[:name].to_s } %>
    <%= f.input :email, :required => true, :autofocus => true, input_html: { value: params[:email].to_s } %>
    <%= f.input :password, :required => true %>
    <%= f.input :password_confirmation, :required => true %>
    <%= f.input :address %>
    <%= f.input :city, input_html: { value: params[:location].to_s.split(",").first } %>
    <%= f.input :state, input_html: { value: params[:location].to_s.split(',').last } %>
    <%= f.input :zip %>
      <%= f.association :role, :prompt =>"----select----" %>
    <%= f.input :avatar %>
    <%= f.input :cover_photo %>

  </div>

  <div class="form-actions">
    <%= f.button :submit, "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
